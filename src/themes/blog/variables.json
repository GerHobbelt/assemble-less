{
  "@black"                                  : "#000",
  "@grayDarker"                             : "#262829",
  "@grayDark"                               : "#333",
  "@gray"                                   : "#555",
  "@grayLight"                              : "#a2a8a8",
  "@grayLighter"                            : "#eee",
  "@grayLightest"                           : "#f5f5f5",
  "@white"                                  : "#fff",
  "@blue"                                   : "#0c7e9e",
  "@blueDark"                               : "darken('@blue, 15%)",
  "@purple"                                 : "#7a43b6",
  "@pink"                                   : "#ef2553",
  "@red"                                    : "lighten(#9d261d, 10%)",
  "@redOrange"                              : "#dd461d",
  "@orange"                                 : "#dd461d",
  "@yellowOrange"                           : "#fca639",
  "@yellow"                                 : "#f0a403",
  "@green"                                  : "#6cc644",

  "@body-background"                        : "#fff",
  "@text-color"                             : "@grayDark",
