{
  "name": "Bootstrap",
  "version": "3.0.0",
  "repo": "https://github.com/twitter/bootstrap.git",
  "root": "./src",

  "images": {
    "all": "<%= bootstrap.root.img %>/**/*.{ico,png,jpg,gif}"
  },
  "fonts": {
    "all" : "<%= bootstrap.root.fonts %>/**/*.{eot,otf,ttf,svg}"
  },

  "variables"           : "<%= bootstrap.less.base %>/variables.less",
  "mixins"              : "<%= bootstrap.less.base %>/mixins.less",

  "less": {
    "base"                : "<%= bootstrap.root %>/libs/bootstrap",
    "dest"                : "<%= bootstrap.root %>/libs/bootstrap/tmp/",
    "accordion"           : "<%= bootstrap.less.base %>/accordion.less",
    "alerts"              : "<%= bootstrap.less.base %>/alerts.less",
    "bootstrap"           : "<%= bootstrap.less.base %>/bootstrap.less",
    "breadcrumbs"         : "<%= bootstrap.less.base %>/breadcrumbs.less",
    "button_groups"       : "<%= bootstrap.less.base %>/button_groups.less",
    "buttons"             : "<%= bootstrap.less.base %>/buttons.less",
    "carousel"            : "<%= bootstrap.less.base %>/carousel.less",
    "close"               : "<%= bootstrap.less.base %>/close.less",
    "code"                : "<%= bootstrap.less.base %>/code.less",
    "component_animations": "<%= bootstrap.less.base %>/component_animations.less",
    "counters"            : "<%= bootstrap.less.base %>/counters.less",
    "dropdowns"           : "<%= bootstrap.less.base %>/dropdowns.less",
    "forms"               : "<%= bootstrap.less.base %>/forms.less",
    "glyphicons"          : "<%= bootstrap.less.base %>/glyphicons.less",
    "grid"                : "<%= bootstrap.less.base %>/grid.less",
    "jumbotron"           : "<%= bootstrap.less.base %>/jumbotron.less",
    "media"               : "<%= bootstrap.less.base %>/media.less",
    "modals"              : "<%= bootstrap.less.base %>/modals.less",
    "navbar"              : "<%= bootstrap.less.base %>/navbar.less",
    "navs"                : "<%= bootstrap.less.base %>/navs.less",
    "normalize"           : "<%= bootstrap.less.base %>/normalize.less",
    "pager"               : "<%= bootstrap.less.base %>/pager.less",
    "pagination"          : "<%= bootstrap.less.base %>/pagination.less",
    "popovers"            : "<%= bootstrap.less.base %>/popovers.less",
    "progress_bars"       : "<%= bootstrap.less.base %>/progress_bars.less",
    "scaffolding"         : "<%= bootstrap.less.base %>/scaffolding.less",
    "tables"              : "<%= bootstrap.less.base %>/tables.less",
    "thumbnails"          : "<%= bootstrap.less.base %>/thumbnails.less",
    "tooltip"             : "<%= bootstrap.less.base %>/tooltip.less",
    "type"                : "<%= bootstrap.less.base %>/type.less",
    "wells"               : "<%= bootstrap.less.base %>/wells.less",

    "utilities"           : "<%= bootstrap.less.base %>/utilities.less",
    "responsive_utilities": "<%= bootstrap.less.base %>/responsive_utilities.less",

    "all": [
      "<%= bootstrap.less.core %>",
      "<%= bootstrap.less.common %>",
      "<%= bootstrap.less.nav %>",
      "<%= bootstrap.less.zindex %>",
      "<%= bootstrap.less.misc %>",
      "<%= bootstrap.less.utilities %>"
    ],

    "core": [
      "<%= bootstrap.less.normalize %>",
      "<%= bootstrap.less.scaffolding %>",
      "<%= bootstrap.less.grid %>",
      "<%= bootstrap.less.type %>",
      "<%= bootstrap.less.code %>",
      "<%= bootstrap.less.forms %>",
      "<%= bootstrap.less.tables %>",
      "<%= bootstrap.less.buttons %>"
    ],
    "common": [
      "<%= bootstrap.less.component_animations %>",
      "<%= bootstrap.less.glyphicons %>",
      "<%= bootstrap.less.dropdowns %>",
      "<%= bootstrap.less.wells %>",
      "<%= bootstrap.less.close %>"
    ],
    "nav": [
      "<%= bootstrap.less.navs %>",
      "<%= bootstrap.less.navbar %>",
      "<%= bootstrap.less.button_groups %>",
      "<%= bootstrap.less.breadcrumbs %>",
      "<%= bootstrap.less.pagination %>",
      "<%= bootstrap.less.pager %>"
    ],
    "zindex": [
      "<%= bootstrap.less.modals %>",
      "<%= bootstrap.less.tooltip %>",
      "<%= bootstrap.less.popovers %>"
    ],
    "misc": [
      "<%= bootstrap.less.alerts %>",
      "<%= bootstrap.less.thumbnails %>",
      "<%= bootstrap.less.media %>",
      "<%= bootstrap.less.counters %>",
      "<%= bootstrap.less.progress_bars %>",
      "<%= bootstrap.less.accordion %>",
      "<%= bootstrap.less.carousel %>",
      "<%= bootstrap.less.jumbotron %>"
    ],
    "util": [
      "<%= bootstrap.less.utilities %>",
      "<%= bootstrap.less.responsive_utilities %>"
    ],
    "custom_bundle": []
  },

  "js": {
    "base"       : "<%= bootstrap.root %>/docs/assets/js",
    "application": "<%= bootstrap.js.base %>/application.js",

    "transition" : "<%= bootstrap.js.base %>/bootstrap_transition.js",
    "modal"      : "<%= bootstrap.js.base %>/bootstrap_modal.js",
    "dropdown"   : "<%= bootstrap.js.base %>/bootstrap_dropdown.js",
    "scrollspy"  : "<%= bootstrap.js.base %>/bootstrap_scrollspy.js",
    "tab"        : "<%= bootstrap.js.base %>/bootstrap_tab.js",
    "tooltip"    : "<%= bootstrap.js.base %>/bootstrap_tooltip.js",
    "popovers"   : "<%= bootstrap.js.base %>/bootstrap_popover.js",
    "affix"      : "<%= bootstrap.js.base %>/bootstrap_affix.js",
    "alerts"     : "<%= bootstrap.js.base %>/bootstrap_alert.js",
    "button"     : "<%= bootstrap.js.base %>/bootstrap_button.js",
    "collapse"   : "<%= bootstrap.js.base %>/bootstrap_collapse.js",
    "carousel"   : "<%= bootstrap.js.base %>/bootstrap_carousel.js",
    "typeahead"  : "<%= bootstrap.js.base %>/bootstrap_typeahead.js",

    "custom_bundle": []
  },

  "all": {
    "assets": [
      "<%= bootstrap.images %>",
      "<%= bootstrap.fonts %>",
      "!<%= bootstrap.ignore %>"
    ],
    "less": [
      "<%= bootstrap.less.base %>/*.less",
      "!<%= bootstrap.ignore %>"
    ],
    "less_another_variation": [
      "<%= bootstrap.less.core %>",
      "<%= bootstrap.less.common %>",
      "<%= bootstrap.less.nav %>",
      "<%= bootstrap.less.zindex %>",
      "<%= bootstrap.less.misc %>",
      "<%= bootstrap.less.utilities %>",
      "!<%= bootstrap.ignore %>"
    ],
    "js": [
      "<%= bootstrap.js.transition %>",
      "<%= bootstrap.js.modal %>",
      "<%= bootstrap.js.dropdowns %>",
      "<%= bootstrap.js.scrollspy %>",
      "<%= bootstrap.js.tab %>",
      "<%= bootstrap.js.tooltip %>",
      "<%= bootstrap.js.popovers %>",
      "<%= bootstrap.js.affix %>",
      "<%= bootstrap.js.alerts %>",
      "<%= bootstrap.js.button %>",
      "<%= bootstrap.js.collapse %>",
      "<%= bootstrap.js.carousel %>",
      "<%= bootstrap.js.typeahead %>",
      "!<%= bootstrap.ignore %>"
    ],
    "files": [
      "<%= bootstrap.images %>",
      "<%= bootstrap.fonts %>",
      "<%= bootstrap.all.less %>",
      "<%= bootstrap.all.js %>",
      "!<%= bootstrap.ignore %>"
    ]
  },

  "ignore": [
    "<%= bootstrap.root %>/**/tests**",
    "<%= bootstrap.less.bootstrap %>",
    "<%= bootstrap.root %>/{make*,component.json,package.json}"
  ]
}

